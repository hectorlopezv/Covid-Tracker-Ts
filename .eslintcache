      await fetch(url)\n            .then((response: any) => response.json())\n            .then(data => {\n                let chartData = buildChartData(data.timeline?? data, casesType);\n                const newCopy = _.cloneDeep(chartData);\n                setdata(newCopy);\n            });\n        };\n        fetchData();\n    }, [casesType, country]);\n    return ( \n        <div>\n        {data?.length > 0 &&\n            <Line\n                \n                data={{\n                datasets: [\n                    {\n                    backgroundColor: \"rgba(204, 16, 52, 0.5)\",\n                    borderColor: \"#CC1034\",\n                    data: data,\n                    },\n                ],\n                }}\n                options={options}\n            />\n        } \n\n        </div>\n     );\n}\n \nexport default LineGraph;","/home/hvmlopez/covid-tracker-redux-saga/src/utils/showData.tsx",[],"/home/hvmlopez/covid-tracker-redux-saga/src/utils/prettyStat.tsx",[],"/home/hvmlopez/covid-tracker-redux-saga/src/utils/build_chartdata.ts",[],{"ruleId":"60","severity":1,"message":"61","line":6,"column":27,"nodeType":"62","messageId":"63","endLine":6,"endColumn":42},{"ruleId":"64","replacedBy":"65"},{"ruleId":"66","replacedBy":"67"},{"ruleId":"60","severity":1,"message":"68","line":1,"column":16,"nodeType":"62","messageId":"63","endLine":1,"endColumn":24},{"ruleId":"60","severity":1,"message":"69","line":2,"column":37,"nodeType":"62","messageId":"63","endLine":2,"endColumn":43},{"ruleId":"60","severity":1,"message":"70","line":2,"column":45,"nodeType":"62","messageId":"63","endLine":2,"endColumn":50},{"ruleId":"60","severity":1,"message":"71","line":2,"column":63,"nodeType":"62","messageId":"63","endLine":2,"endColumn":74},{"ruleId":"60","severity":1,"message":"72","line":2,"column":76,"nodeType":"62","messageId":"63","endLine":2,"endColumn":88},{"ruleId":"60","severity":1,"message":"73","line":1,"column":37,"nodeType":"62","messageId":"63","e