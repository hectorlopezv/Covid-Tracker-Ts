       setcountries([...countries]);\n        });\n\n    }\n    getCountriesData();\n  }, []);\n\n  return (\n    <div className=\"app\">\n\n      <div className=\"app_left\">\n        <div className=\"app_header\">\n          <h1>Covid Tracker</h1>\n          <FormControl className=\"app_dropdown\">\n            <Select variant=\"outlined\" value={country}\n              onChange={onCountyChange}>\n              <MenuItem value=\"worldwide\">Worldwide</MenuItem>\n              {countries.map((country: any) => <MenuItem\n                value={country.name}\n                key={country.name}\n              >{country.name}</MenuItem>)}\n            </Select>\n          </FormControl>\n        </div>\n\n\n        <div className=\"app_stats\">\n          <InfoBox\n            title=\"Coronavirus Cases\"\n            cases={contryInfo.todayCases}\n            total={contryInfo.cases}\n          />\n\n          <InfoBox\n            title=\"Recovered\"\n            cases={contryInfo.todayRecovered}\n            total={contryInfo.recovered}\n          />\n\n          <InfoBox\n            title=\"Deaths\"\n            cases={contryInfo.todayDeaths}\n            total={contryInfo.deaths}\n          />\n        </div>\n\n        <Map \n          center={mapCenter}\n          zoom={mapZoom}\n        />\n      </div>\n\n      <Card className=\"app_right\">\n        <CardContent>\n          <h3>Live Cases By County</h3>\n          {/*table*/}\n          <Table countries={tableData}>\n\n          </Table>\n\n          <h3>Worldwide new cases</h3>\n          <LineGraph \n            casesType=\"cases\"\n          />\n        </CardContent>\n      </Card>\n    </div>\n  );\n}\n\nexport default App;\n","/home/hvmlopez/covid-tracker-redux-saga/src/components/InfoBox/InfoBox.tsx",[],"/home/hvmlopez/covid-tracker-redux-saga/src/components/map/Map.tsx",[],"/home/hvmlopez/covid-tracker-redux-saga/src/components/Table/Table.tsx",[],"/home/hvmlopez/covid-tracker-redux-saga/src/utils/sor